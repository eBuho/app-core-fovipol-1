	<queryString language="SQL">
		<![CDATA[SELECT
     TRM_TRAMITE."CODI_EXPE_TRM" AS EXPEDIENTE,
     TRM_TRAMITE."FECH_INGR_TRM" AS FECHA,
     MAE_SOCIO."CODI_CCIP_SOC" AS CIP,
     MAE_PERSONA."APEL_PATE_PER" AS APELLIDO_PATERNO,
     MAE_PERSONA."APEL_MATE_PER" AS APELLIDO_MATERNO,
     MAE_PERSONA."NOMB_PERS_PER" AS NOMBRES,
     CRD_CREDITO."IMPO_SOLI_CRD" AS MONTO,
     CRD_CREDITO."FECH_APRO_CRD" AS FECHA_APROBADO,
     CRD_CREDITO."TASA_INTE_CRD" AS TEA,
     CRD_CREDITO."NUME_CUOT_PRD" AS PLAZO,
     CRD_CREDITO."USUA_CREA_AUD" AS SECTORISTA,
     MAE_PRODUCTO."NOMB_PROD_PRD" AS PRODUCTO,
     MAE_ENTIDADDET."VALO_CADU_DET" AS Moneda
FROM
     "SIFO"."MAE_PERSONA" MAE_PERSONA INNER JOIN "SIFO"."MAE_SOCIO" MAE_SOCIO ON MAE_PERSONA."IDEN_PERS_PER" = MAE_SOCIO."IDEN_PERS_PER"
     INNER JOIN "SIFO"."TRM_TRAMITE" TRM_TRAMITE ON MAE_PERSONA."IDEN_PERS_PER" = TRM_TRAMITE."CODI_PERS_TRM"
     INNER JOIN "SIFO"."CRD_CREDITO" CRD_CREDITO ON TRM_TRAMITE."IDEN_EXPE_TRM" = CRD_CREDITO."IDEN_EXPE_TRM"
     INNER JOIN "SIFO"."CRD_SIMULACION" CRD_SIMULACION ON MAE_SOCIO."IDEN_PERS_PER" = CRD_SIMULACION."IDEN_PERS_PER"
     INNER JOIN "SIFO"."MAE_PRODUCTO" MAE_PRODUCTO ON CRD_SIMULACION."IDEN_PROD_PRD" = MAE_PRODUCTO."IDEN_PROD_PRD"
     INNER JOIN "SIFO"."MAE_ENTIDADDET" MAE_ENTIDADDET ON MAE_PRODUCTO."CODI_MONE_CRD" = MAE_ENTIDADDET."SECU_ENTI_DET"
WHERE MAE_ENTIDADDET.CODI_ENTI_ENT = 'CODIMONECRD']]>
	</queryString>
