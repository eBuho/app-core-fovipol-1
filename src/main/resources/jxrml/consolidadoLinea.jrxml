	<queryString language="SQL">
		<![CDATA[SELECT
     to_char(CRD_CREDITO."FECH_APRO_CRD", 'Month') AS Mes,
     MAE_ENTIDADDET."VALO_CADU_DET" AS Linea,
     count(*) As Nro_Prestamos,
     sum(CRD_CREDITO."IMPO_SOLI_CRD") AS Monto
FROM
     "SIFO"."TRM_TRAMITE" TRM_TRAMITE INNER JOIN "SIFO"."CRD_CREDITO" CRD_CREDITO ON TRM_TRAMITE."IDEN_EXPE_TRM" = CRD_CREDITO."IDEN_EXPE_TRM"
     INNER JOIN "SIFO"."CRD_SIMULACION" CRD_SIMULACION ON TRM_TRAMITE."IDEN_SIMU_SIM" = CRD_SIMULACION."IDEN_SIMU_SIM"
     INNER JOIN "SIFO"."MAE_PRODUCTO" MAE_PRODUCTO ON CRD_SIMULACION."IDEN_PROD_PRD" = MAE_PRODUCTO."IDEN_PROD_PRD"
     INNER JOIN "SIFO"."MAE_ENTIDADDET" MAE_ENTIDADDET ON MAE_PRODUCTO."CODI_LINE_PRD" = MAE_ENTIDADDET."SECU_ENTI_DET"
WHERE MAE_ENTIDADDET.CODI_ENTI_ENT = 'CODILINEPRD'
GROUP BY to_char(CRD_CREDITO."FECH_APRO_CRD", 'Month'), MAE_ENTIDADDET."VALO_CADU_DET"]]>
	</queryString>
